<?xml version="1.0"?>
<!DOCTYPE project>

<project name="build-common-shared">
	<import file="../build-common-plugin.xml" />

	<target name="jar">
		<jar-macro
			module.dir="."
		/>
	</target>

	<target name="javadoc">
		<antcall target="build-common-plugin.javadoc">
			<param name="javac.classpathref" value="plugin.classpath" />
			<param name="javac.srcdir" value="src" />
		</antcall>
	</target>
</project>