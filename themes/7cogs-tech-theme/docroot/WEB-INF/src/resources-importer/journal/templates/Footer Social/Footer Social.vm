<h2>$title.data</h2>
<h4>$tagline.data</h4>
<ul class="social">
	#foreach ($social-link in $social-links.siblings)
		<li>
			<a alt="${social-link.data}" href="${social-link.url-location.data}" target="new_window" title="${social-link.data}"><img src="${social-link.icon.data}" /><span>${social-link.data}</span></a>
		</li>
	#end
</ul>