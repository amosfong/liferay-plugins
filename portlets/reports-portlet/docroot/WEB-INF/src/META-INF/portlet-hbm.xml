<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.reports.model.Definition" />
	<import class="com.liferay.reports.model.Entry" />
	<import class="com.liferay.reports.model.Source" />
	<class name="com.liferay.reports.model.impl.DefinitionImpl" table="Reports_Definition">
		<cache usage="read-write" />
		<id name="definitionId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="description" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="sourceId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="reportName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="reportParameters" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.reports.model.impl.EntryImpl" table="Reports_Entry">
		<cache usage="read-write" />
		<id name="entryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="definitionId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="format" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="scheduleRequest" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="startDate" type="org.hibernate.type.TimestampType" />
		<property name="endDate" type="org.hibernate.type.TimestampType" />
		<property name="repeating" type="com.liferay.portal.dao.orm.hibernate.BooleanType" />
		<property name="recurrence" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailNotifications" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailDelivery" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="portletId" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="pageURL" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="reportParameters" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="errorMessage" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.reports.model.impl.SourceImpl" table="Reports_Source">
		<cache usage="read-write" />
		<id name="sourceId" type="long">
			<generator class="assigned" />
		</id>
		<property name="uuid" type="com.liferay.portal.dao.orm.hibernate.StringType" column="uuid_" />
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="driverClassName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="driverUrl" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="driverUserName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="driverPassword" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>