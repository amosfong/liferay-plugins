<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.bbb.model.MeetingEntry" />
	<import class="com.liferay.bbb.model.MeetingParticipant" />
	<import class="com.liferay.bbb.model.MeetingServer" />
	<class name="com.liferay.bbb.model.impl.MeetingEntryImpl" table="BBB_MeetingEntry">
		<cache usage="read-write" />
		<id name="meetingEntryId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="meetingServerId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="attendeePassword" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="moderatorPassword" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="status" type="com.liferay.portal.dao.orm.hibernate.IntegerType" />
	</class>
	<class name="com.liferay.bbb.model.impl.MeetingParticipantImpl" table="BBB_MeetingParticipant">
		<cache usage="read-write" />
		<id name="meetingParticipantId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="meetingEntryId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="emailAddress" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
	<class name="com.liferay.bbb.model.impl.MeetingServerImpl" table="BBB_MeetingServer">
		<cache usage="read-write" />
		<id name="meetingServerId" type="long">
			<generator class="assigned" />
		</id>
		<property name="groupId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="companyId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userId" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property name="userName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="createDate" type="org.hibernate.type.TimestampType" />
		<property name="modifiedDate" type="org.hibernate.type.TimestampType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="url" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="secret" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>