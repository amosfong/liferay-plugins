<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
>
	<bean id="com.liferay.oauth.OAuthUtil" class="com.liferay.oauth.util.OAuthUtil">
		<property name="OAuth" ref="com.liferay.oauth.util.V10aOAuth" />
	</bean>
	<bean class="com.liferay.oauth.util.V10aOAuth">
		<constructor-arg>
			<bean class="com.liferay.oauth.util.DefaultOAuthValidator" />
		</constructor-arg>
	</bean>
</beans>