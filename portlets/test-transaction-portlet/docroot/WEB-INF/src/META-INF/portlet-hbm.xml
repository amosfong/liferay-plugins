<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false" auto-import="false">
	<import class="com.liferay.testtransaction.model.Bar" />
	<class name="com.liferay.testtransaction.model.impl.BarImpl" table="TestTransaction_Bar">
		<cache usage="read-write" />
		<id name="barId" type="long">
			<generator class="assigned" />
		</id>
		<property name="text_" access="com.liferay.portal.dao.orm.hibernate.EscapedPropertyAccessor" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>