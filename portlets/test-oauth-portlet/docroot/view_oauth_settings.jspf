<liferay-ui:header title="1.-set-oauth-consumer-credentials" />

<liferay-ui:icon image="checked" />

<p>
	<liferay-ui:message arguments="<%= hostName %>" key="host-name-{0}" /><br />
	<liferay-ui:message arguments="<%= hostPort %>" key="host-port-{0}" /><br />
	<liferay-ui:message arguments="<%= accessTokenURI %>" key="access-token-{0}" /><br />
	<liferay-ui:message arguments="<%= authorizeURI %>" key="authorize-uri-{0}" /><br />
	<liferay-ui:message arguments="<%= requestTokenURI %>" key="request-uri-{0}" /><br />
	<liferay-ui:message arguments="<%= key %>" key="consumer-key-{0}" /><br />
	<liferay-ui:message arguments="<%= secret %>" key="consumer-secret-{0}" />
</p>

<c:if test="<%= Validator.isNotNull(accessToken) %>">
	<liferay-ui:header title="2.-obtain-oauth-accessor-credentials" />

	<liferay-ui:icon image="checked" />

	<p>
		<liferay-ui:message arguments="<%= accessToken %>" key="access-token-{0}" /><br />
		<liferay-ui:message arguments="<%= accessSecret %>" key="access-secret-{0}" />
	</p>
</c:if>